{
  "name": "positive-ui",
  "private": false,
  "version": "0.0.8",
  "main": "es/index.js",
  "module": "dist/positive-components.esm.js",
  "files": [
    "es/*"
  ],
  "keywords": [
    "components",
    "antd",
    "react"
  ],
  "scripts": {
    "tsc:es": "tsc --declarationDir es",
    "tsc:lib": "tsc --declarationDir lib",
    "tsc": "npm run tsc:lib && npm run tsc:es",
    "clean": "rimraf lib es dist tsconfig.tsbuildinfo",
    "test": "vitest",
    "coverage": "vitest run --coverage",
    "vite": "vite",
    "build:only": "vite build",
    "build": "npm run clean && npm run tsc && vite build"
  },
  "dependencies": {
    "react": ">=18.2.0",
    "react-dom": "^18.2.0"
  },
  "peerDependencies": {
    "antd": ">=5.3.2",
    "react": ">=18.2.0"
  },
  "devDependencies": {
    "@types/node": "^18.15.5",
    "@types/react": "^18.0.28",
    "@types/react-dom": "^18.0.11",
    "@vitejs/plugin-react": "^3.1.0",
    "antd": "^5.3.2",
    "decamelize": "^6.0.0",
    "rimraf": "^4.4.0",
    "sass": "^1.59.3",
    "typescript": "^4.9.3",
    "vite": "^4.2.0",
    "vite-plugin-build": "^0.7.1",
    "vitest": "^0.29.7"
  },
  "license": "ISC"
}
